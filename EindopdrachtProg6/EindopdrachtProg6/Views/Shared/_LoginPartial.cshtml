@if (Request.IsAuthenticated) {
    <text>
        Hello, @User.Identity.Name!
        @Html.ActionLink("Logout", "Logout", "Start", routeValues: null, htmlAttributes: new { id = "logoutLink" })


    </text>
} else {
    <text>
        @Html.ActionLink("Login", "Login", "Start", routeValues: null, htmlAttributes: new { id = "loginLink" })
    </text>
}
